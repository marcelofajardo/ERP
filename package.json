{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "knowledge-base-build": "webpack",
        "knowledge-base-production": "SET NODE_ENV=production webpack && rm -f ./public/dist/*styles.js",
        "knowledge-base-build-profile": "SET NODE_ENV=production webpack --profile --json > webpack-stats.json && rm -f ./public/dist/*styles.js",
        "knowledge-base-dev": "npm-run-all --parallel watch livereload",
        "knowledge-base-watch": "webpack --watch",
        "knowledge-base-livereload": "livereload ./public/dist/",
        "knowledge-base-permissions": "chown -R $USER:$USER bootstrap/cache storage public/uploads"
    },
    "devDependencies": {
        "axios": "^0.18",
        "bootstrap": "^4.3.1",
        "jquery": "^3.4.1",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.14.7",
        "vue": "^2.5.7",
        "css-loader": "^2.1.1",
        "livereload": "^0.8.0",
        "mini-css-extract-plugin": "^0.7.0",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2"
    },
    "dependencies": {
        "bootstrap-sass": "^3.3.7",
        "chart.js": "^2.8.0",
        "clipboard": "^2.0.4",
        "codemirror": "^5.47.0",
        "cross-env": "^5.2.0",
        "dropzone": "^5.5.1",
        "laravel-echo": "^1.4.0",
        "livechatapi": "^0.6.0",
        "luxon": "^1.15.0",
        "markdown-it": "^8.4.2",
        "markdown-it-task-lists": "^2.1.1",
        "popper": "^1.0.1",
        "pusher-js": "^4.3.1",
        "readmore-js": "^2.2.1",
        "sortablejs": "^1.9.0",
        "vue": "^2.6.10",
        "vue-chartjs": "^3.4.2",
        "vue-datetime": "^1.0.0-beta.10",
        "vue-resource": "^1.5.1",
        "vuedraggable": "^2.21.0",
        "weekstart": "^1.0.0"
    },
    "browser": {
        "vue": "vue/dist/vue.common.js "
    }
}
